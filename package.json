		"@tsconfig/svelte": "^5.0.2",
		"@types/d3": "^7.4.3",
		"d3": "^7.9.0",
		"moment": "^2.30.1",
		"sass": "^1.83.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.5.0",
		"uikit": "^3.19.1",
		"vite": "^5.4.4"
	},
	"type": "module"
